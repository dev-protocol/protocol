{
	"name": "@devprtcl/protocol",
	"version": "2.0.0",
	"description": "Securitize for Internet assets",
	"main": "cli/index.js",
	"scripts": {
		"test": "truffle test",
		"generate": "truffle compile && typechain --target truffle './build/**/*.json'",
		"deploy": "truffle migrate --network",
		"predeploy": "yarn build",
		"lint": "yarn lint:eslint && yarn lint:solhint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:solhint": "solhint --fix --max-warnings 0 \"contracts/**/*.sol\"",
		"lint:format": "prettier --write '**/*.{sol,js,json,md,yml}'",
		"prepack": "yarn build",
		"build": "tsc -p tsconfig.build.json",
		"prebuild": "rimraf ['migrations/**/*.js', 'scripts/**/*.js'] && yarn generate"
	},
	"bin": {
		"dev-protocol": "cli/index.js"
	},
	"files": [
		"contracts",
		"migrations",
		"truffle-config.js"
	],
	"author": "aggre",
	"license": "MPL-2.0",
	"devDependencies": {
		"@truffle/hdwallet-provider": "1.2.0",
		"@types/bent": "7.3.2",
		"@types/dotenv": "8.2.0",
		"@types/node": "13.13.14",
		"@types/shelljs": "0.8.8",
		"@typescript-eslint/eslint-plugin": "4.8.1",
		"@typescript-eslint/parser": "4.8.1",
		"bent": "7.3.12",
		"bignumber.js": "9.0.1",
		"dotenv": "8.2.0",
		"eslint": "7.13.0",
		"eslint-config-prettier": "6.15.0",
		"eslint-config-xo": "0.33.1",
		"eslint-config-xo-typescript": "0.35.0",
		"eslint-plugin-prettier": "3.1.4",
		"husky": "4.3.0",
		"p-queue": "6.6.2",
		"prettier": "2.1.2",
		"prettier-plugin-solidity": "1.0.0-beta.1",
		"rimraf": "3.0.2",
		"solhint": "3.3.2",
		"truffle": "5.1.54",
		"truffle-typings": "1.0.8",
		"ts-generator": "0.1.1",
		"ts-node": "9.0.0",
		"typechain": "3.0.0",
		"typechain-target-truffle": "1.0.2",
		"typescript": "4.1.2",
		"solhint": "3.3.2",
    "@devprtcl/utils": "1.2.1",
		"web3": "1.3.0"
	},
	"dependencies": {
		"@openzeppelin/contracts": "^2.5.0",
		"commander": "^6.0.0",
		"shelljs": "^0.8.3"
	},
	"bundleddependencies": [
		"truffle",
		"shelljs",
		"commander",
		"dotenv",
		"ts-node",
		"truffle-hdwallet-provider"
	],
	"directories": {
		"doc": "docs",
		"test": "test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dev-protocol/protocol.git"
	},
	"bugs": {
		"url": "https://github.com/dev-protocol/protocol/issues"
	},
	"homepage": "https://github.com/dev-protocol/protocol#readme",
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint"
		}
	}
}
